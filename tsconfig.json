{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "./dist/",
    "sourceMap": true,
    "declaration": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "resolveJsonModule": true,
    "lib": [
      "es6",
      "dom"
    ],
  },
  "exclude": [
    "dist",
    "node_modules",
    "test-cases",
    "nodes"
  ]
}